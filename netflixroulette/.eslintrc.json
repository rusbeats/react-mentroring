{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "import",
        "jsx-a11y"
    ],
    "rules": {
        "comma-dangle": "off",
        "react/jsx-filename-extension": "off",
        "no-use-before-define":"off",
        "react/prop-types": "off",
        "jsx-a11y/label-has-for":"off",
        "linebreak-style": "off"
    }
}